{
  "name": "@agrzes/yellow-next-core",
  "version": "0.0.1",
  "description": "",
  "main": "target",
  "scripts": {
    "test": "c8 mocha test/**/*.{ts,tsx}",
    "start": "node target",
    "build": "rm -rf target types && tsc",
    "watch": "tsc --watch --preserveWatchOutput"
  },
  "types": "types",
  "type": "module",
  "c8": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ]
  },
  "mocha": {
    "require": [
      "ts-node/esm",
      "source-map-support/register"
    ],
    "node-option": [
      "experimental-specifier-resolution=node",
      "loader=ts-node/esm"
    ]
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT",
  "files": [
    "target",
    "types",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "registry": "http://npm.kubernetes.home.agrzes.pl/"
  },
  "dependencies": {
    "@agrzes/yellow-next-web": "workspace:^",
    "@mdx-js/loader": "^3.0.1",
    "chokidar": "^3.6.0",
    "commander": "^12.0.0",
    "connect-history-api-fallback": "^2.0.0",
    "debug": "^4.3.4",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "gray-matter": "^4.0.3",
    "html-webpack-plugin": "^5.6.0",
    "inversify": "^6.0.2",
    "js-yaml": "^4.1.0",
    "jsonld": "^8.3.2",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.8.0",
    "n3": "^1.17.2",
    "pouchdb": "^8.0.1",
    "pouchdb-adapter-memory": "^8.0.1",
    "reflect-metadata": "^0.2.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx-frontmatter": "^4.0.0",
    "rxjs": "^7.8.1",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.1",
    "webpack": "^5.0.0",
    "webpack-dev-middleware": "^7.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonld": "^1.5.13",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/n3": "^1.16.4",
    "@types/node": "^20.11.16",
    "@types/pouchdb": "^6.4.2",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "c8": "^9.1.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "bin": {
    "yd": "bin/cli.mjs"
  }
}