{
  "name": "@agrzes/yellow-next-shared",
  "description": "",
  "version": "0.0.1",
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "c8": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "@inrupt/vocab-common-rdf": "1.0.5",
    "handlebars": "4.7.8",
    "jsonld": "8.3.2",
    "lodash": "4.17.21",
    "n3": "1.17.3",
    "reflect-metadata": "0.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.12",
    "@types/chai-as-promised": "^7.1.8",
    "@types/chai-subset": "^1.3.5",
    "@types/jsonld": "^1.5.13",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/n3": "^1.16.4",
    "@types/node": "^20.11.25",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "c8": "^9.1.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "files": [
    "target",
    "types",
    "LICENSE",
    "package.json"
  ],
  "license": "MIT",
  "main": "src/index.tsx",
  "mocha": {
    "require": [
      "ts-node/esm",
      "source-map-support/register"
    ],
    "node-option": [
      "experimental-specifier-resolution=node",
      "loader=ts-node/esm"
    ]
  },
  "publishConfig": {
    "registry": "http://npm.kubernetes.home.agrzes.pl/"
  },
  "scripts": {
    "build": "rm -rf target types && tsc",
    "start": "node target",
    "test": "c8 mocha test/**/*.{ts,tsx}",
    "watch": "tsc --watch --preserveWatchOutput"
  },
  "type": "module",
  "types": "types",
  "exports": {
    "./dynamic/access": {
      "types": "./types/dynamic/access/index.d.ts",
      "default": "./target/dynamic/access/index.js"
    },
    "./dynamic/mapper": {
      "types": "./types/dynamic/mapper/index.d.ts",
      "default": "./target/dynamic/mapper/index.js"
    },
    "./dynamic/semantic": {
      "types": "./types/dynamic/semantic.d.ts",
      "default": "./target/dynamic/semantic.js"
    },
    "./dynamic/model": {
      "types": "./types/dynamic/model.d.ts",
      "default": "./target/dynamic/model.js"
    }
  },
  "typesVersions": {
    "*": {
      "dynamic/access": [
        "types/dynamic/access/index.d.ts"
      ],
      "dynamic/mapper": [
        "types/dynamic/mapper/index.d.ts"
      ],
      "dynamic/semantic": [
        "types/dynamic/semantic.d.ts"
      ],
      "dynamic/model": [
        "types/dynamic/model.d.ts"
      ]
    }
  }
}