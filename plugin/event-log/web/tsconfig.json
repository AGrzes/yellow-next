{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "strictNullChecks": true,
    "jsx": "react"
  },
  "ts-node": {
    "moduleTypes": {
      "*.ts": "cjs"
    }
  }
}
