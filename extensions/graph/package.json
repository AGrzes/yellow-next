{
  "name": "@agrzes/yellow-next-extensions-graph",
  "description": "",
  "version": "0.0.1",
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "c8": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "@agrzes/yellow-next-shared": "workspace:*",
    "@agrzes/yellow-next-web": "workspace:*",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@mui/material": "5.15.15",
    "lodash": "4.17.23",
    "luxon": "^3.5.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.3",
    "reagraph": "^4.20.1",
    "typescript": "5.4.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.2.64"
  },
  "peerDependencies": {
    "react": "18.2.0"
  },
  "files": [
    "target",
    "types",
    "LICENSE",
    "package.json"
  ],
  "license": "MIT",
  "main": "src/index.tsx",
  "mocha": {
    "require": [
      "ts-node/esm",
      "source-map-support/register"
    ],
    "node-option": [
      "experimental-specifier-resolution=node",
      "loader=ts-node/esm"
    ]
  },
  "publishConfig": {
    "registry": "http://npm.kubernetes.home.agrzes.pl/"
  },
  "scripts": {
    "build": "rm -rf target types && tsc",
    "start": "node target",
    "watch": "tsc --watch --preserveWatchOutput"
  },
  "type": "module",
  "types": "types",
  "exports": {
    ".": "./src/index.tsx"
  },
  "typesVersions": {
    "*": {}
  }
}