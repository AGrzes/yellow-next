{
  "name": "@agrzes/yellow-next-core",
  "description": "",
  "version": "0.0.1",
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "bin": {
    "yd": "bin/cli.mjs"
  },
  "c8": {
